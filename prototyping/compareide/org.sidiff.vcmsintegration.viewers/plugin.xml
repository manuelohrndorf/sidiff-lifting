<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="org.sidiff.vcmsintegration.structureview.silift"
         point="org.eclipse.compare.structureMergeViewers">
      <viewer
      		extensions="ecore"
            class="org.sidiff.vcmsintegration.structureview.SiLiftStructureMergeViewerCreator"
            id="org.sidiff.vcmsintegration.structureview.silift"
            label="SiLiftViewer">
      </viewer>
      <contentTypeBinding
            contentTypeId="org.eclipse.emf.ecore"
            structureMergeViewerId="org.sidiff.vcmsintegration.structureview.silift">
      </contentTypeBinding>
      <contentTypeBinding
            contentTypeId="org.eclipse.uml2.uml"
            structureMergeViewerId="org.sidiff.vcmsintegration.structureview.silift">
      </contentTypeBinding>
   </extension>
   <extension
         id="org.sidiff.vcmsintegration.contentview.silift"
         point="org.eclipse.compare.contentMergeViewers">
      <viewer
            class="org.sidiff.vcmsintegration.contentview.SiLiftContentMergeViewerCreator"
            extensions="ecore"
            id="org.sidiff.vcmsintegration.contentview.silift"
            label="SiLiftViewer">
      </viewer>
      <contentTypeBinding
            contentMergeViewerId="org.sidiff.vcmsintegration.contentview.silift"
            contentTypeId="org.eclipse.emf.ecore">
      </contentTypeBinding>
      <contentTypeBinding
            contentMergeViewerId="org.sidiff.vcmsintegration.contentview.silift"
            contentTypeId="org.eclipse.uml2.uml">
      </contentTypeBinding>
   </extension>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="org.eclipse.compare.internal.CompareEditor"
            class="org.sidiff.vcmsintegration.properties.CompareEditorPropertySheetPageAdapterFactory">
         <adapter
               type="org.eclipse.ui.views.properties.IPropertySheetPage">
         </adapter>
      </factory>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
      <propertyContributor
            contributorId="org.sidiff.vcmsintegration.compareeditor">
         <propertyCategory
               category="org.sidiff.vcmsintegration.compareeditor.category.default"></propertyCategory>
         <propertyCategory
               category="org.sidiff.vcmsintegration.compareeditor.category.operation">
         </propertyCategory>
      </propertyContributor>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections
            contributorId="org.sidiff.vcmsintegration.compareeditor">
         <propertySection
               class="org.sidiff.vcmsintegration.properties.DefaultPropertySection"
               id="org.sidiff.vcmsintegration.compareeditor.section.default"
               tab="org.sidiff.vcmsintegration.compareeditor.tab.default">
            <input
                  type="java.lang.Object">
            </input>
         </propertySection>
         <propertySection
               class="org.sidiff.patching.ui.view.InputParameterSection"
               id="org.sidiff.vcmsintegration.compareeditor.section.operation.arguments.input"
               tab="org.sidiff.vcmsintegration.compareeditor.tab.operation.arguments.input">
            <input
                  type="org.sidiff.patching.operation.OperationInvocationWrapper">
            </input>
         </propertySection>
         <propertySection
               class="org.sidiff.patching.ui.view.OutputParameterSection"
               id="org.sidiff.vcmsintegration.compareeditor.section.operation.arguments.output"
               tab="org.sidiff.vcmsintegration.compareeditor.tab.operation.arguments.output">
            <input
                  type="org.sidiff.patching.operation.OperationInvocationWrapper">
            </input>
         </propertySection>
      </propertySections>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs
            contributorId="org.sidiff.vcmsintegration.compareeditor">
         <propertyTab
               category="org.sidiff.vcmsintegration.compareeditor.category.default"
               id="org.sidiff.vcmsintegration.compareeditor.tab.default"
               label="Default">
         </propertyTab>
         <propertyTab
               afterTab="org.sidiff.vcmsintegration.compareeditor.tab.default"
               category="org.sidiff.vcmsintegration.compareeditor.category.operation"
               id="org.sidiff.vcmsintegration.compareeditor.tab.operation.arguments.input"
               label="Input">
         </propertyTab>
         <propertyTab
               afterTab="org.sidiff.vcmsintegration.compareeditor.tab.operation.arguments.input"
               category="org.sidiff.vcmsintegration.compareeditor.category.operation"
               id="org.sidiff.vcmsintegration.compareeditor.tab.operation.arguments.output"
               label="Output">
         </propertyTab>
      </propertyTabs>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.ui.views.PropertySheet">
         <command
               commandId="org.sidiff.patching.ui.showQualifiedArgumentsCommand"
               icon="icons/url.gif"
               style="toggle"
               tooltip="Enables qualified argument names">
            <visibleWhen
                  checkEnabled="false">
               <reference
                     definitionId="org.sidiff.vcmsintegration.compareeditor.PropertyView">
               </reference>
            </visibleWhen>
         </command>
         <command
               commandId="org.sidiff.patching.ui.ShowReliabilitiesCommand"
               icon="icons/reliability.gif"
               style="toggle"
               tooltip="Enables reliability values of the matching result">
            <visibleWhen
                  checkEnabled="false">
               <reference
                     definitionId="org.sidiff.vcmsintegration.compareeditor.PropertyView">
               </reference>
            </visibleWhen>
         </command>
         <command
               commandId="org.sidiff.patching.ui.showNonDefaultArguments"
               icon="icons/filter_changed_attributes.gif"
               style="toggle"
               tooltip="Enables the visibility of non-defaulting arguments">
            <visibleWhen
                  checkEnabled="false">
               <reference
                     definitionId="org.sidiff.vcmsintegration.compareeditor.PropertyView">
               </reference>
            </visibleWhen>
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="org.sidiff.vcmsintegration.compareeditor.PropertyView">
         <with
               variable="activeEditorId">
            <equals
                  value="org.eclipse.compare.CompareEditor">
            </equals>
         </with>
      </definition>
   </extension>

</plugin>
