            category="org.sidiff.integration.properties.SiDiffSettings"
            class="org.sidiff.integration.preferences.domains.DomainSettingsIndexPage"
            id="org.sidiff.integration.properties.SiDiffSettings.domains"
            name="Domains">
         <enabledWhen>
            <reference
                  definitionId="org.sidiff.integration.preferences.showPropertiesDefinition">
            </reference>
