         point="org.eclipse.ui.preferencePages">
      <page
            category="org.sidiff.integration.preferences.SiDiffSettings"
            class="org.sidiff.integration.preferences.domains.DomainSettingsIndexPage"
            id="org.sidiff.integration.preferences.SiDiffSettings.domains"
            name="Domains">
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.propertyPages">
