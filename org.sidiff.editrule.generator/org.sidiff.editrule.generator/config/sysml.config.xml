<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Config SYSTEM "http://pi.informatik.uni-siegen.de/SiDiff/Editrulesgeneratorconfig.dtd" >
<Config>
	<GeneralSettings>
		<preventInconsistency value="true" />
		<multiplicityPreconditions integrated="false" separately="false" />
		<reduceToSuperType
			ADD_REMOVE="true"
			CHANGE_LITERAL="true"
			CHANGE_REFERENCE="true"
			CREATE_DELETE="true"
			MOVE="true"
			MOVE_DOWN="true"
			MOVE_UP="true"
			SET_UNSET_ATTRIBUTE="true"
			SET_UNSET_REFERENCE="true" />
		<disableVariantsWithSupertypeReplacement value="true"/>
		<createAllAttributes value="true" />
	</GeneralSettings>
	
	<MetaModelSettings>
		<MainModel nsUri="http://www.eclipse.org/papyrus/0.7.0/SysML" />
		<RequiredModel nsUri="http://www.eclipse.org/emf/2002/Ecore" />
		<RequiredModel nsUri="http://www.eclipse.org/uml2/4.0.0/UML" />
		<RequiredModel nsUri="http://www.eclipse.org/uml2/4.0.0/Types" />	
		<!-- 	
		<Constraints>
			<NameUniqueness scope="local" eClassifier="NamedElement" eAttributeName="name" eAttributeIsInherited="false"  overrideInheritedConstraintsIfAny="false" applyOnSubTypes="true"/>
		</Constraints>
		 -->
		<MaskedClassifiers>
		</MaskedClassifiers>
	</MetaModelSettings>

	
	<ProfiledModelSettings>
		<BaseModelRules path="/org.sidiff.sysml.editrules.atomic/transformation/generated/unprofiled"/>	
	</ProfiledModelSettings>
	
	<Transformations>
		<Creates allow="true" />
		<Deletes allow="true" />
		<Moves allow="true" allowReferenceSwitching="true" allowReferenceCombinations="true" />
		<MoveUps allow="true"/>
		<MoveDowns allow="true"/>
		<SetAttributes allow="true" />
		<UnsetAttributes allow="true" />
		<SetReferences allow="true" />
		<UnsetReferences allow="true" />
		<Adds allow="true" />
		<Removes allow="true" />
		<ChangeLiterals allow="true" allowLiteralSwitching="true" />
		<ChangeReferences allow="true" />
	</Transformations>

	<!-- nested=true: means the root element can also appear
	somewhere in its lower containments (eg. (Sub-)StateMachine)-->
	<Root name="Model" nested="false"/>
	
	<WhiteList>			

	<EClassifier name="Block" />
	<EClassifier name="FlowPort" />
	<EClassifier name="ConstraintProperty" />
	<EClassifier name="ConstraintBlock" />
	<EClassifier name="RequirementRelated" />
	<EClassifier name="Requirement" />
	<EClassifier name="Satisfy" />	
	
	
	</WhiteList>
	
	<BlackList>

    		
	</BlackList>
	
</Config>