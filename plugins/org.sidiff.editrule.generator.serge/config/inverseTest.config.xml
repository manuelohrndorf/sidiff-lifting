<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Config SYSTEM "http://pi.informatik.uni-siegen.de/SiDiff/Editrulesgeneratorconfig.dtd" >
<Config>
	<GeneralSettings>
		<preventInconsistency value="true" />
		<multiplicityPreconditions integrated="true" separately="true" />
		<reduceToSuperType
			ADD_REMOVE="false"
			CHANGE_LITERAL="false"
			CHANGE_REFERENCE="false"
			CREATE_DELETE="true"
			MOVE="false"
			MOVE_DOWN="false"
			MOVE_UP="false"
			SET_UNSET_ATTRIBUTE="false"
			SET_UNSET_REFERENCE="false" />
		<disableVariantsWithSupertypeReplacement value="false" />
		<createAllAttributes value="true" />		
   		<modelUsesProfileMechanism value="false"/>
	</GeneralSettings>
	
	<MetaModelSettings>
		<MainModel nsUri="http://www.eclipse.org/emf/2002/Ecore" />		
		<Constraints/>
		<!-- 
		<Constraints>
			<NameUniqueness scope="local" eClassifier="ENamedElement" eAttributeName="name" eAttributeIsInherited="false"  overrideInheritedConstraintsIfAny="false" applyOnSubTypes="true"/>
		</Constraints>	 
		-->
	</MetaModelSettings>
	
	<Transformations>
		<Creates allow="true" />
		<Deletes allow="true" />
		<Moves allow="false" allowReferenceSwitching="false" allowReferenceCombinations="false" />
		<MoveUps allow="false"/>
		<MoveDowns allow="false"/>
		<SetAttributes allow="false" />
		<UnsetAttributes allow="false" />
		<SetReferences allow="false" />
		<UnsetReferences allow="false" />
		<Adds allow="false" />
		<Removes allow="false" />
		<ChangeLiterals allow="false" allowLiteralSwitching="true" />
		<ChangeReferences allow="false" />
	</Transformations>

	<!-- nested=true: means the root element can also appear
	somewhere in its lower containments. -->
	<Root name="EPackage" nested="true"/>
	
	<WhiteList>
		<EClassifier name="EPackage" />
	</WhiteList>
	
	<BlackList>
	</BlackList>
	
</Config>