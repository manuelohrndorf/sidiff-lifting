<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Config SYSTEM "http://pi.informatik.uni-siegen.de/SiDiff/Editrulesgeneratorconfig.dtd" >
<Config>
	<GeneralSettings>
		<preventInconsistency value="true" />
		<multiplicityPreconditions integrated="true"
			separately="false" />
		<reduceToSuperType
			ADD_REMOVE="false"
			CHANGE_LITERAL="false"
			CHANGE_REFERENCE="false"
			CREATE_DELETE="false"
			MOVE="false"
			MOVE_DOWN="false"
			MOVE_UP="false"
			SET_UNSET_ATTRIBUTE="false"
			SET_UNSET_REFERENCE="false" />
		<disableVariantsWithSupertypeReplacement value="true" />
		<modelUsesProfileMechanism value="false" />
		<createAllAttributes value="true" />
	</GeneralSettings>
	<MetaModelSettings>
		<MainModel nsUri="http://www.eclipse.org/emf/2011/Henshin" />
		<RequiredModel nsUri="http://www.eclipse.org/emf/2002/Ecore" />	
	</MetaModelSettings>

	<Transformations>
		<Creates allow="true" />
		<Deletes allow="true" />
		<Moves allow="true" allowReferenceSwitching="true" allowReferenceCombinations="true" />
		<MoveUps allow="false"/>
		<MoveDowns allow="false"/>
		<SetAttributes allow="true" />
		<UnsetAttributes allow="false" />
		<SetReferences allow="true" />
		<UnsetReferences allow="false" />
		<Adds allow="true" />
		<Removes allow="true" />
		<ChangeLiterals allow="true" allowLiteralSwitching="true" />
		<ChangeReferences allow="true" />
	</Transformations>
	
	<!-- The root object must be specified by the user actually -->
	<Root name="" nested="true" />
	<WhiteList>
	
	</WhiteList>
	<BlackList>	

	</BlackList>
</Config>
