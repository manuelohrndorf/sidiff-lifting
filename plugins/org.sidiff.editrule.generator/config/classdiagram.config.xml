<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Config SYSTEM "http://pi.informatik.uni-siegen.de/SiDiff/Editrulesgeneratorconfig.dtd" >
<Config>
	<GeneralSettings>
		<preventInconsistency value="true" />
		<multiplicityPreconditions integrated="false" separately="false" />
		<reduceToSuperType
			ADD_REMOVE="true"
			CHANGE_LITERAL="true"
			CHANGE_REFERENCE="true"
			CREATE_DELETE="true"
			MOVE="true"
			MOVE_DOWN="true"
			MOVE_UP="true"
			SET_UNSET_ATTRIBUTE="true"
			SET_UNSET_REFERENCE="true" />
		<disableVariantsWithSupertypeReplacement value="false" />
		<createAllAttributes value="true" />		
		<modelUsesProfileMechanism value="false" />
	</GeneralSettings>

	<MetaModelSettings>
		<MainModel nsUri="http://www.eclipse.org/uml2/4.0.0/UML" />
		<RequiredModel nsUri="http://www.eclipse.org/uml2/4.0.0/Types" />
		<RequiredModel nsUri="http://www.eclipse.org/emf/2002/Ecore" />	
		<!-- 
		<Constraints>
			<NameUniqueness scope="local" eClassifier="NamedElement" eAttributeName="name" eAttributeIsInherited="false" overrideInheritedConstraintsIfAny="false" applyOnSubTypes="true"/>
		</Constraints>
		 -->
		<MaskedClassifiers>	
		</MaskedClassifiers>
	</MetaModelSettings>

	<Transformations>
		<Creates allow="true" />
		<Deletes allow="true" />
		<Moves allow="true" allowReferenceSwitching="true" allowReferenceCombinations="true" />
		<MoveUps allow="true"/>
		<MoveDowns allow="true"/>
		<SetAttributes allow="true" />
		<UnsetAttributes allow="true" />
		<SetReferences allow="true" />
		<UnsetReferences allow="true" />
		<Adds allow="true" />
		<Removes allow="true" />
		<ChangeLiterals allow="true" allowLiteralSwitching="true" />
		<ChangeReferences allow="true" />
	</Transformations>

	<Root name="Model" nested="false" />

	<WhiteList>

		<EClassifier name="Abstraction" />
		<EClassifier name="Association" />
		<EClassifier name="Class" />
		<EClassifier name="Enumeration" />
		<EClassifier name="EnumerationLiteral" />	
		<EClassifier name="Generalization" />
		<EClassifier name="LiteralBoolean" />
		<EClassifier name="LiteralString" />
		<EClassifier name="Operation" />		
		<EClassifier name="Package" />
		<EClassifier name="Parameter" />
		<EClassifier name="Port" />
		<EClassifier name="ProfileApplication" />
		<EClassifier name="Property" />	
		<EClassifier name="Type" />
		
	</WhiteList>
	<BlackList>
  
	
  	</BlackList>
</Config>