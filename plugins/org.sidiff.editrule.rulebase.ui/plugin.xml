<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="column" name="Rulebase Column" schema="schema/column.exsd"/>

   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="org.sidiff.editrule.rulebase.ui.definition.rulebaseEditor">
         <with
               variable="activeEditorId">
            <equals
                  value="org.sidiff.editrule.rulebase.ui.rulebaseEditor">
            </equals>
         </with>
      </definition>
   </extension>
   <extension
         point="org.sidiff.editrule.rulebase.ui.column">
      <column
            id="HEAD"></column>
      <column
            id="APPEND"></column>
      <column
            class="org.sidiff.editrule.rulebase.ui.editor.columns.impl.ColumnActive"
            id="org.sidiff.editrule.rulebase.ui.ColumnActive"
            insertAfter="HEAD">
      </column>
      <column
            class="org.sidiff.editrule.rulebase.ui.editor.columns.impl.ColumnRulebaseItem"
            id="org.sidiff.editrule.rulebase.ui.ColumnRulebaseItem"
            insertAfter="org.sidiff.editrule.rulebase.ui.ColumnActive"></column>
      <column
            class="org.sidiff.editrule.rulebase.ui.editor.columns.impl.ColumnDescription"
            id="org.sidiff.editrule.rulebase.ui.ColumnDescription"
            insertAfter="org.sidiff.editrule.rulebase.ui.ColumnRulebaseItem">
      </column>
      <column
            class="org.sidiff.editrule.rulebase.ui.editor.columns.impl.ColumnEditType"
            id="org.sidiff.editrule.rulebase.ui.ColumnEditType"
            insertAfter="org.sidiff.editrule.rulebase.ui.ColumnDescription">
      </column>
      <column
            class="org.sidiff.editrule.rulebase.ui.editor.columns.impl.ColumnDerivedRefrences"
            id="org.sidiff.editrule.rulebase.ui.ColumnDerivedRefrences"
            insertAfter="org.sidiff.editrule.rulebase.ui.ColumnEditType">
      </column>
      <column
            class="org.sidiff.editrule.rulebase.ui.editor.columns.impl.ColumnDependenyCount"
            id="org.sidiff.editrule.rulebase.ui.ColumnDependenyCount"
            insertAfter="org.sidiff.editrule.rulebase.ui.ColumnDerivedRefrences">
      </column>
      <column
            class="org.sidiff.editrule.rulebase.ui.editor.columns.impl.ColumnNameBasedClassification"
            id="org.sidiff.editrule.rulebase.ui.ColumnNameBasedClassification"
            insertAfter="org.sidiff.editrule.rulebase.ui.ColumnDependenyCount">
      </column>
      <column
            class="org.sidiff.editrule.rulebase.ui.editor.columns.impl.ColumnInverseRule"
            id="org.sidiff.editrule.rulebase.ui.ColumnInverseRule"
            insertAfter="org.sidiff.editrule.rulebase.ui.ColumnNameBasedClassification">
      </column>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.sidiff.editrule.rulebase.ui.editor.RulebaseEditor"
            contributorClass="org.sidiff.editrule.rulebase.ui.editor.RulebaseActionBarContributor"
            default="false"
            extensions="rulebase"
            icon="icons/rulebase.gif"
            id="org.sidiff.editrule.rulebase.ui.rulebaseEditor"
            name="Rulebase Editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
      <propertyContributor
            contributorId="org.sidiff.editrule.rulebase.ui.rulebaseEditor">
         <propertyCategory
               category="org.sidiff.editrule.rulebase.ui.properties.category.default"></propertyCategory>
         <propertyCategory
               category="org.sidiff.editrule.rulebase.ui.properties.category.dependencies">
         </propertyCategory>
         <propertyCategory
               category="org.sidiff.editrule.rulebase.ui.properties.category.conflicts">
         </propertyCategory>
      </propertyContributor>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs
            contributorId="org.sidiff.editrule.rulebase.ui.rulebaseEditor">
         <propertyTab
               category="org.sidiff.editrule.rulebase.ui.properties.category.default"
               id="org.sidiff.editrule.rulebase.ui.properties.tab.default"
               label="Default">
         </propertyTab>
         <propertyTab
               category="org.sidiff.editrule.rulebase.ui.properties.category.dependencies"
               id="org.sidiff.editrule.rulebase.ui.properties.tab.dependencies"
               label="Dependencies">
         </propertyTab>
         <propertyTab
               category="org.sidiff.editrule.rulebase.ui.properties.category.conflicts"
               id="org.sidiff.editrule.rulebase.ui.properties.tab.conflicts"
               label="Conflicts">
         </propertyTab>
      </propertyTabs>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections
            contributorId="org.sidiff.editrule.rulebase.ui.rulebaseEditor">
         <propertySection
               class="org.sidiff.editrule.rulebase.ui.editor.properties.DefaultPropertySection"
               id="org.sidiff.editrule.rulebase.ui.properties.section.default"
               tab="org.sidiff.editrule.rulebase.ui.properties.tab.default">
            <input
                  type="java.lang.Object">
            </input>
         </propertySection>
         <propertySection
               class="org.sidiff.editrule.rulebase.ui.editor.properties.PotentialDependenciesSection"
               id="org.sidiff.editrule.rulebase.ui.properties.section.dependencies"
               tab="org.sidiff.editrule.rulebase.ui.properties.tab.dependencies">
            <input
                  type="java.lang.Object">
            </input>
         </propertySection>
         <propertySection
               class="org.sidiff.editrule.rulebase.ui.editor.properties.PotentialConflictsSection"
               id="org.sidiff.editrule.rulebase.ui.properties.section.conflicts"
               tab="org.sidiff.editrule.rulebase.ui.properties.tab.conflicts">
            <input
                  type="java.lang.Object">
            </input>
         </propertySection>
      </propertySections>
   </extension>
</plugin>
