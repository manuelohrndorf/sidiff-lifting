<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Config SYSTEM "http://pi.informatik.uni-siegen.de/SiDiff/ProfileApplicatorConfig.dtd" >
<Config>
	<ProfileSettings>
		<Profile name="SysML" />
		<BaseTypeInstances allow="true" />
		<BaseTypeInheritance allow="true" />
	</ProfileSettings>

	<Packages>
		<BasePackage nsUri="http://www.eclipse.org/uml2/5.0.0/UML" />
		<StereoPackage nsUri="http://www.eclipse.org/papyrus/sysml/1.4/SysML" />
	</Packages>

	<Transformations>
		<Transformation name="CREATE" apply="true" />
		<Transformation name="DELETE" apply="true" />
		<Transformation name="PRESERVE" apply="true" />
	</Transformations>
	<StereoTypes>
	<!--  Used stereotypes in PP-Unit , comment out if all stereotypes 
	of the SysML profile should be generated-->
	 	<StereoType name="Block" />
		<StereoType name="FlowPort" />
		<StereoType name="ConstraintProperty" />
		<StereoType name="ConstraintBlock" />
		<!-- <StereoType name="RequirementRelated" /> -->
		<StereoType name="Requirement" />
		<StereoType name="Satisfy" />
	</StereoTypes>

</Config>