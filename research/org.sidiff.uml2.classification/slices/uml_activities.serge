				<GenerateModulesFor type="InterruptibleActivityRegion" case="always" />
				<GenerateModulesFor type="ExecutableNode" case="always" />
				<GenerateModulesFor type="ExceptionHandler" case="always" />
				<GenerateModulesFor type="ObjectNode" case="always" />
				<GenerateModulesFor type="ObjectNodeOrderingKind" case="always" />
				<GenerateModulesFor type="Variable" case="always" />
				<GenerateModulesFor type="ExpansionKind" case="always" />
				<GenerateModulesFor type="ActivityFinalNode" case="always" />
				<GenerateModulesFor type="FinalNode" case="always" />
				<GenerateModulesFor type="ControlNode" case="always" />
				<GenerateModulesFor type="ActivityParameterNode" case="always" />
				<GenerateModulesFor type="CentralBufferNode" case="always" />
