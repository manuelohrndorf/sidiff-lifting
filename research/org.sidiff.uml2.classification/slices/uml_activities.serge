				<DanglingNodeSetting type="ActivityPartition" include="always" />
				<DanglingNodeSetting type="ActivityNode" include="always" />
				<DanglingNodeSetting type="InterruptibleActivityRegion" include="always" />
				<DanglingNodeSetting type="ExecutableNode" include="always" />
				<DanglingNodeSetting type="ExceptionHandler" include="always" />
				<DanglingNodeSetting type="ObjectNode" include="always" />
				<DanglingNodeSetting type="ObjectNodeOrderingKind" include="always" />
				<DanglingNodeSetting type="Variable" include="always" />
				<DanglingNodeSetting type="ExpansionKind" include="always" />
				<DanglingNodeSetting type="ActivityFinalNode" include="always" />
				<DanglingNodeSetting type="FinalNode" include="always" />
