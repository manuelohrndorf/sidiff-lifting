				<DanglingNodeSetting type="CentralBufferNode" include="always" />
				<DanglingNodeSetting type="ControlFlow" include="always" />
				<DanglingNodeSetting type="DataStoreNode" include="always" />
				<DanglingNodeSetting type="DecisionNode" include="always" />
				<DanglingNodeSetting type="ObjectFlow" include="always" />
				<DanglingNodeSetting type="FlowFinalNode" include="always" />
				<DanglingNodeSetting type="ForkNode" include="always" />
				<DanglingNodeSetting type="InitialNode" include="always" />
				<DanglingNodeSetting type="JoinNode" include="always" />
				<DanglingNodeSetting type="MergeNode" include="always" />
				<DanglingNodeSetting type="MessageKind" include="always" />
				<DanglingNodeSetting type="MessageSort" include="always" />
				<DanglingNodeSetting type="InteractionOperatorKind" include="always" />
			</DanglingNodeSettings>
		</ModuleInternalOptions>

		<ModuleSetOptions>
			<ModuleGenerationSettings
				defaultGenerationCase="ifRequired">
				<GenerateModulesFor type="Namespace" case="always" />
				<GenerateModulesFor type="Activity" case="always" />
				<GenerateModulesFor type="Behavior" case="always" />
				<GenerateModulesFor type="Class" case="always" />
				<GenerateModulesFor type="BehavioredClassifier" case="always" />
				<GenerateModulesFor type="Classifier" case="always" />
				<GenerateModulesFor type="Namespace" case="always" />
				<GenerateModulesFor type="NamedElement" case="always" />
				<GenerateModulesFor type="Element" case="always" />
				<GenerateModulesFor type="PackageableElement" case="always" />
				<GenerateModulesFor type="ParameterableElement" case="always" />
				<GenerateModulesFor type="TemplateableElement" case="always" />
				<GenerateModulesFor type="VisibilityKind" case="always" />
				<GenerateModulesFor type="Type" case="always" />
				<GenerateModulesFor type="ConnectableElement" case="always" />
				<GenerateModulesFor type="TypedElement" case="always" />
				<GenerateModulesFor type="MultiplicityElement" case="always" />
				<GenerateModulesFor type="ParameterDirectionKind" case="always" />
				<GenerateModulesFor type="ParameterEffectKind" case="always" />
