<?xml version="1.0" encoding="ASCII"?>
<henshin:Module xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" xmlns:henshin="http://www.eclipse.org/emf/2011/Henshin" name="REMOVE_Vertex_(outgoing)_TGT_Transition" description="Removes Vertex's reference outgoing the target Transition">
  <annotations key="INVERSE" value="ADD_Vertex_(outgoing)_TGT_Transition"/>
  <imports href="http://www.sidiff.org/model/statemachine#/"/>
  <units xsi:type="henshin:Rule" name="removeFromVertex_outgoing_Transition" description="Removes from Vertex's reference outgoing the target Transition">
    <parameters name="Selected">
      <type xsi:type="ecore:EClass" href="http://www.sidiff.org/model/statemachine#//Vertex"/>
    </parameters>
    <parameters name="OldTarget">
      <type xsi:type="ecore:EClass" href="http://www.sidiff.org/model/statemachine#//Transition"/>
    </parameters>
    <lhs name="LHS">
      <nodes name="Selected" incoming="//@units.0/@lhs/@edges.1" outgoing="//@units.0/@lhs/@edges.0">
        <type href="http://www.sidiff.org/model/statemachine#//Vertex"/>
      </nodes>
      <nodes name="OldTarget" incoming="//@units.0/@lhs/@edges.0" outgoing="//@units.0/@lhs/@edges.1">
        <type href="http://www.sidiff.org/model/statemachine#//Transition"/>
      </nodes>
      <edges source="//@units.0/@lhs/@nodes.0" target="//@units.0/@lhs/@nodes.1">
        <type href="http://www.sidiff.org/model/statemachine#//Vertex/outgoing"/>
      </edges>
      <edges source="//@units.0/@lhs/@nodes.1" target="//@units.0/@lhs/@nodes.0">
        <type href="http://www.sidiff.org/model/statemachine#//Transition/source"/>
      </edges>
    </lhs>
    <rhs name="RHS">
      <nodes name="Selected">
        <type href="http://www.sidiff.org/model/statemachine#//Vertex"/>
      </nodes>
      <nodes name="OldTarget">
        <type href="http://www.sidiff.org/model/statemachine#//Transition"/>
      </nodes>
    </rhs>
    <mappings origin="//@units.0/@lhs/@nodes.0" image="//@units.0/@rhs/@nodes.0"/>
    <mappings origin="//@units.0/@lhs/@nodes.1" image="//@units.0/@rhs/@nodes.1"/>
  </units>
  <units xsi:type="henshin:PriorityUnit" name="mainUnit" subUnits="//@units.0">
    <parameters name="selectedEObject">
      <annotations key="INVERSE" value="selectedEObject"/>
      <type xsi:type="ecore:EClass" href="http://www.sidiff.org/model/statemachine#//Vertex"/>
    </parameters>
    <parameters name="OldTarget">
      <type xsi:type="ecore:EClass" href="http://www.sidiff.org/model/statemachine#//Transition"/>
    </parameters>
    <parameterMappings source="//@units.1/@parameters.0" target="//@units.0/@parameters.0"/>
    <parameterMappings source="//@units.1/@parameters.1" target="//@units.0/@parameters.1"/>
  </units>
</henshin:Module>
