<?xml version="1.0" encoding="ASCII"?>
<henshin:Module xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" xmlns:henshin="http://www.eclipse.org/emf/2011/Henshin" name="ADD_PseudoState_(incoming)_TGT_Transition" description="Adds to PseudoState's reference incoming the target Transition">
  <annotations key="INVERSE" value="REMOVE_PseudoState_(incoming)_TGT_Transition"/>
