      <type xsi:type="ecore:EClass" href="http://www.sidiff.org/model/statemachine#//PseudoState"/>
    </parameters>
    <parameters name="NewTarget">
      <type xsi:type="ecore:EClass" href="http://www.sidiff.org/model/statemachine#//Transition"/>
    </parameters>
    <lhs name="Lhs">
