<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Config SYSTEM "http://pi.informatik.uni-siegen.de/SiDiff/Editrulesgeneratorconfig.dtd" >
<Config>
	<GeneralSettings>
		<preventInconsistency value="true" />
		<multiplicityPreconditions integrated="true" separately="true" />
		<reduceToSuperType SET_UNSET="true" ADD_REMOVE="true" CHANGE="true" />
		<disableVariants value="true"/>
   		<modelUsesProfileMechanism value="true"/>
	</GeneralSettings>
	
	<MetaModelSettings>
		<MainModel nsUri="http://www.eclipse.org/papyrus/0.7.0/SysML" />
		<RequiredModel nsUri="http://www.eclipse.org/emf/2002/Ecore" />
		<RequiredModel nsUri="http://www.eclipse.org/uml2/4.0.0/UML" />
		<RequiredModel nsUri="http://www.eclipse.org/uml2/4.0.0/Types" />		
		<Constraints>
			<NameUniqueness scope="local" eClass="NamedElement" eAttributeName="name" eAttributeIsInherited="false"  overrideInheritedConstraintsIfAny="false" applyOnSubTypes="true"/>
		</Constraints>
		<MaskedClassifiers>
		</MaskedClassifiers>
	</MetaModelSettings>

	
	<ProfiledModelSettings>
		<BaseModelRules path="/org.sidiff.sysml.editrules.atomic/transformation/generated"/>	
	</ProfiledModelSettings>
	
	<Transformations>
		<Creates allow="true" />
		<Deletes allow="true" />
		<Moves allow="true" allowReferenceSwitching="true" />
		<Sets allow="true" />
		<Unsets allow="true" />
		<Adds allow="true" />
		<Removes allow="true" />
		<Changes allow="true" allowLiteralSwitching="true"/>
	</Transformations>

	<!-- nested=true: means the root element can also appear
	somewhere in its lower containments (eg. (Sub-)StateMachine)-->
	<Root name="Model" nested="false"/>
	
	<WhiteList>			

	
	</WhiteList>
	
	<BlackList>

    		
	</BlackList>
	
</Config>