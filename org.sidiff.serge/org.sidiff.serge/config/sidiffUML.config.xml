<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Config SYSTEM "http://pi.informatik.uni-siegen.de/SiDiff/Editrulesgeneratorconfig.dtd" >
<Config>
	<GeneralSettings>
		<preventInconsistency value="true" />
		<multiplicityPreconditions integrated="true" separately="true" />
		<reduceToSuperType  CREATE_DELETE="true" MOVE="true" SET_UNSET="true" ADD_REMOVE="true" CHANGE="true" />
		<disableVariants value="true" />
		<createAllAttributes value="true" />		
		<modelUsesProfileMechanism value="false" />
	</GeneralSettings>

	<MetaModelSettings>
		<MainModel nsUri="http://www.sidiff.org/org.sidiff.uml.model" />
		<Constraints>
			<NameUniqueness scope="local" eClassifier="UMLNamedElement" eAttributeName="name" eAttributeIsInherited="false"  overrideInheritedConstraintsIfAny="false" applyOnSubTypes="true"/>
			<NameUniqueness scope="global" eClassifier="UMLPackage" eAttributeName="name" eAttributeIsInherited="true"  overrideInheritedConstraintsIfAny="true" applyOnSubTypes="false"/>
		</Constraints>	
		
	</MetaModelSettings>

	<Transformations>
		<Creates allow="true" />
		<Deletes allow="true" />
		<Moves allow="true" allowReferenceSwitching="true" />
		<Sets allow="true" />
		<Unsets allow="true" />
		<Adds allow="true" />
		<Removes allow="true" />
		<Changes allow="true" allowLiteralSwitching="true"/>
	</Transformations>

	<Root name="UMLModel" nested="false" />

	<WhiteList>


	</WhiteList>
	<BlackList>
  
	
  	</BlackList>
</Config>