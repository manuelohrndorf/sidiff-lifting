<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Config SYSTEM "http://pi.informatik.uni-siegen.de/SiDiff/Editrulesgeneratorconfig.dtd" >
<Config>
	<GeneralSettings>
		<preventInconsistency value="true" />
		<multiplicityPreconditions integrated="true" separately="false" />
		<reduceToSuperType  CREATE_DELETE="true" MOVE="true" SET_UNSET="true" ADD_REMOVE="true" CHANGE="true" />
		<disableVariants value="true" />
		<modelUsesProfileMechanism value="false" />
	</GeneralSettings>

	<MetaModelSettings>
		<MainModel nsUri="http://www.eclipse.org/uml2/4.0.0/UML" />
		<RequiredModel nsUri="http://www.eclipse.org/uml2/4.0.0/Types" />
		<RequiredModel nsUri="http://www.eclipse.org/emf/2002/Ecore" />	
		<Constraints>
			<NameUniqueness scope="local" eClass="NamedElement" eAttributeName="name" eAttributeIsInherited="false" overrideInheritedConstraintsIfAny="false" applyOnSubTypes="true"/>
			<NameUniqueness scope="local" eClass="Package" eAttributeName="name" eAttributeIsInherited="true" overrideInheritedConstraintsIfAny="true" applyOnSubTypes="true"/>
		</Constraints>
		<MaskedClassifiers>	
		</MaskedClassifiers>
	</MetaModelSettings>

	<Transformations>
		<Creates allow="true" />
		<Deletes allow="true" />
		<Moves allow="true" allowReferenceSwitching="false" />
		<Sets allow="true" />
		<Unsets allow="true" />
		<Adds allow="true" />
		<Removes allow="true" />
		<Changes allow="true" allowLiteralSwitching="false"/>
	</Transformations>

	<Root name="Model" nested="false" />

	<WhiteList>

		<EClass name="Class" />
		<EClass name="Package" />
		<EClass name="Model" />
		<EClass name="Operation" />
		<EClass name="Property" />
		<EClass name="Interface" />
		<EClass name="Association" />
		<EClass name="Generalization" />
		<EClass name="Enumeration" />
		<EClass name="EnumerationLiteral" />
		<EClass name="Parameter" />
		<EClass name="Dependency" />
		
		<EClass name="Port" />
		<EClass name="Activity" />
		
	</WhiteList>
	<BlackList>
  
	
  	</BlackList>
</Config>