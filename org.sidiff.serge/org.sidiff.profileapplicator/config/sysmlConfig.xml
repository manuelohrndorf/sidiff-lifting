<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Config SYSTEM "http://pi.informatik.uni-siegen.de/SiDiff/ProfileApplicatorConfig.dtd" >
<Config>

	<ProfileSettings>
	    <Profile name="SysML" />
		<MetaInstances allow="false" />		
	</ProfileSettings>
		
	<Packages>
		<BasePackage nsUri="http://www.eclipse.org/uml2/4.0.0/UML" />	
		<StereoPackage nsUri="http://www.eclipse.org/papyrus/0.7.0/SysML" />
	</Packages>
	
	<Transformations>
		<Transformation name="ADD" apply="true" />
		<Transformation name="CREATE" apply="true" />
		<Transformation name="DELETE" apply="true" />
	</Transformations>
		
	<StereoTypes>
		<StereoType name="Block" baseType="Class" baseReference="base_Class" />			
		<StereoType name="FlowPort" baseType="Port" baseReference="base_Port" />
		<StereoType name="ConstraintProperty" baseType="Property" baseReference="base_Property" />
		<StereoType name="ConstraintBlock" baseType="Class" baseReference="base_Class" />	
		<StereoType name="RequirementRelated" baseType="NamedElement" baseReference="base_NamedElement" />
		<StereoType name="Requirement" baseType="Class" baseReference="base_Class" />			
		<StereoType name="Satisfy" baseType="Abstraction" baseReference="base_Abstraction" /> 
	</StereoTypes>
		
</Config>