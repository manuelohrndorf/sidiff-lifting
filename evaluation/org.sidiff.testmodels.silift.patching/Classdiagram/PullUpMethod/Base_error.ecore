<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="classdiagram"
    nsURI="http://classdiagram/1.0" nsPrefix="classdiagram">
  <eClassifiers xsi:type="ecore:EClass" name="Book" eSuperTypes="#//Borrowable">
    <eOperations name="getDateOfIssue" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Library">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="address" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="borrowables" upperBound="-1"
        eType="#//Borrowable" containment="true" eOpposite="#//Borrowable/library"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DVD" eSuperTypes="#//Borrowable">
    <eOperations name="getDateOfIssue" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Borrowable" abstract="true">
    <eOperations name="getDateOfIssue" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="title" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="library" lowerBound="1"
        eType="#//Library" eOpposite="#//Library/borrowables"/>
  </eClassifiers>
</ecore:EPackage>
