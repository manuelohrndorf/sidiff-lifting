<?xml version="1.0" encoding="ASCII"?>
<henshin:Module xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" xmlns:henshin="http://www.eclipse.org/emf/2011/Henshin" name="DELETE_Parameter_IN_Unit_(parameters)" description="Deletes one Parameter in Unit">
  <annotations key="INVERSE" value="CREATE_Parameter_IN_Unit_(parameters)"/>
  <imports href="http://www.eclipse.org/emf/2011/Henshin#/"/>
  <units xsi:type="henshin:Rule" name="deleteParameter_IN_Unit" description="deletes one Parameter in the context: Unit">
