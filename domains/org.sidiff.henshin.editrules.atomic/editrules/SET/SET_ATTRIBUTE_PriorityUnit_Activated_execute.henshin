<?xml version="1.0" encoding="ASCII"?>
<henshin:Module xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" xmlns:henshin="http://www.eclipse.org/emf/2011/Henshin" name="SET_ATTRIBUTE_PriorityUnit_Activated" description="Sets PriorityUnit Activated">
  <imports href="http://www.eclipse.org/emf/2011/Henshin#/"/>
  <units xsi:type="henshin:Rule" name="setPriorityUnitActivated" description="Sets the EAttribute activated">
    <parameters name="Selected">
      <type xsi:type="ecore:EClass" href="http://www.eclipse.org/emf/2011/Henshin#//PriorityUnit"/>
    </parameters>
    <parameters name="Activated">
      <type xsi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
    </parameters>
    <lhs name="Lhs">
      <nodes name="Selected">
        <type href="http://www.eclipse.org/emf/2011/Henshin#//PriorityUnit"/>
      </nodes>
    </lhs>
    <rhs name="Rhs">
      <nodes name="Selected">
        <type href="http://www.eclipse.org/emf/2011/Henshin#//PriorityUnit"/>
        <attributes value="Activated">
          <type href="http://www.eclipse.org/emf/2011/Henshin#//Unit/activated"/>
        </attributes>
      </nodes>
    </rhs>
    <mappings origin="//@units.0/@lhs/@nodes.0" image="//@units.0/@rhs/@nodes.0"/>
  </units>
  <units xsi:type="henshin:PriorityUnit" name="mainUnit" subUnits="//@units.0">
    <parameters name="selectedEObject">
      <type xsi:type="ecore:EClass" href="http://www.eclipse.org/emf/2011/Henshin#//PriorityUnit"/>
    </parameters>
    <parameters name="Activated">
      <type xsi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
    </parameters>
    <parameterMappings source="//@units.1/@parameters.0" target="//@units.0/@parameters.0"/>
    <parameterMappings source="//@units.1/@parameters.1" target="//@units.0/@parameters.1"/>
  </units>
</henshin:Module>
